{
  "Trip": [
      {{#repeat 10000}}
        {
          "ticker": "{{@guid}}",
          "title": "{{lorem 5}}",
          "description": "{{lorem 15}}",
          "requeriments": "{{lorem 15}}",
          "startDate": "{{date '2010' '2020'}}",
          "endDate": "{{date '2010' '2020'}}",
          "cancelled": {{boolean}},
          "public": {{boolean}},
          "managedBy": "{{@guid}}",
          "stages": [
                  {{#repeat 4}}
                    {
                      "title": "{{city}} ({{country}}) with {{company}}",
                      "description": "{{lorem 15}}",
                      "price": "{{int 0 1000}}",
                      "date": "{{date '2020-01-01' '2021-12-31'}}"
                    }
                  {{/repeat}}
                  ]
        }
    {{/repeat}}
  ],
  "Actor": [
      {{#repeat 10000}}
        {
          "name": "{{firstName}}",
          "surname": "{{lastName}}",
          "email": "{{email}}",
          "password": "{{@guid}}",
          "preferredLanguage": "{{language}}",
          "phone": "{{phone}}",
          "address": "{{int 1 100}} {{street}}",
          "role": "{{role}}",
          "active": {{boolean}},
          "managedBy": "{{@guid}}"
        }
    {{/repeat}}
  ],
"Application": [
      {{#repeat 10000}}
        {
          "status": "{{status}}",
          "paid": {{boolean}},
          "tripId": "{{@guid}}",
          "explorerId": "www.{{company}}.com",
          "comments":[
                      {{#repeat 3}}
                      "{{lorem 15}}"
                      {{/repeat}}
                    ]
        }
    {{/repeat}}
  ],
"Sponsorship": [
      {{#repeat 10000}}
        {
          "sponsorId": "{{@guid}}",
          "tripId": "{{@guid}}",
          "paid": {{boolean}},
          "link": "www.{{company}}.com"
        }
    {{/repeat}}
  ]
 
}

